ist := {'Attack'. 'Use Health Potion'. 'Use Mana Potion' . firstPlayer getSKILL1 getSkillName. firstPlayer getSKILL2 getSkillName}.	chosenMove := UIManager default chooseFrom: moveList message: 'Choose an Move for Player 1'.].n % 2 = 1ifTrue:[	moveList := {'Attack'. 'Use Health Potion'. 'Use Mana Potion'. secondPlayer getSKILL1 getSkillName. secondPlayer getSKILL2 getSkillName}.	chosenMove := UIManager default chooseFrom: moveList message: 'Choose an Move for Player 2'.].chosenMove = 1 & (n raisedTo: 1 modulo: 2)= 0ifTrue:[			self attack: firstPlayer defence: secondPlayer.			Transcript show: 'Player 1'.			self printPlayer: firstPlayer.			self cool1: firstPlayer cool2: secondPlayer.].		chosenMove = 1 & (n raisedTo: 1 modulo: 2) = 1ifTrue:[			self attack: secondPlayer defence: firstPlayer.			Transcript show: 'Player 2'.			self printPlayer: secondPlayer.			self cool1: firstPlayer cool2: secondPlayer.].	chosenMove = 2ifTrue:[	(n raisedTo: 1 modulo: 2) = 0 	ifTrue:[		firstPlayer getHPPot > 0		ifTrue:[Health_Potion player: firstPlayer.				Transcript show: 'Player 1 used Health Potion.'.				self printPlayer: firstPlayer.				self cool1: firstPlayer cool2: secondPlayer.				]		ifFalse:[Transcript cr; show: 'Player 1, No Health Potion!! Try anohter move.'.	 			success1:= -1.].	]	ifFalse:[		secondPlayer getHPPot > 0		ifTrue:[Health_Potion player: secondPlayer.				Transcript show:  'Player 2 used Health Potion'.				self printPlayer: secondPlayer.				self cool1: firstPlayer cool2: secondPlayer.				]		ifFalse:[Transcript cr; show: 'Player 2, No Health Potion!! Try anohter move.'.	 			success1:= -1.].	].].chosenMove = 3ifTrue:[	(n raisedTo: 1 modulo: 2) = 0 	ifTrue:[		firstPlayer getManaPot > 0		ifTrue:[Mana_Potion player: firstPlayer.				Transcript show: 'Player 1 used Mana Potion.'.				self printPlayer: firstPlayer.				self cool1: firstPlayer cool2: secondPlayer.				]		ifFalse:[Transcript cr; show: 'Player 1, No Mana Potion