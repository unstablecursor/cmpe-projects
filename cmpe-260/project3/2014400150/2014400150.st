getSKILL2 attacker: firstPlayer defenser: secondPlayer.					Transcript cr ; show: 'Player 1 used ' , firstPlayer getSKILL2 getSkillName.					self printPlayer: firstPlayer.					self cool1: firstPlayer cool2: secondPlayer.				]			ifTrue:[					Transcript cr; show: 'Player 1, ',  firstPlayer getSKILL2 getSkillName,' is Passive!! Try anohter move.'.	 				success1:= -1.				].				]			ifFalse:[					Transcript cr; show: 'Player 1, ',  firstPlayer getSKILL2 getSkillName,' in cooldown!! Try anohter move.'.	 				success1:= -1.				].			].	(n raisedTo: 1 modulo: 2)= 1	ifTrue:[		secondPlayer getS2Cool = 0			ifTrue:[				secondPlayer getSKILL2 isPassive = 1				ifFalse:[  					secondPlayer getSKILL2 attacker: secondPlayer defenser: firstPlayer.					Transcript cr;  show:  'Player 2 used ', secondPlayer getSKILL2 getSkillName.					self printPlayer: secondPlayer.					self cool1: firstPlayer cool2: secondPlayer.				]				ifTrue:[					Transcript cr; show: 'Player 2, ',  secondPlayer getSKILL2 getSkillName ,' is Passive!! Try anohter move.'.	 				success1:= -1.					].				]			ifFalse:[					Transcript cr; show: 'Player 2, ',  secondPlayer getSKILL2 getSkillName,' in cooldown!! Try anohter move.'.	 				success1:= -1.				].			].		].n := n + 1 + success1.].! !!GameArena class methodsFor: 'as yet unclassified' stamp: 'Deniz 5/19/2017 21:13'!printPlayer: player	Transcript cr;	show: 'Hero: ', player getName; cr;	show: player getHP asString, ' / ' , player getMaxHP asString, ' HP'; cr;	show: player getMANA asString, ' / ' , player getMaxMANA asString, ' MANA'; cr;	show: player getHPPot asString, ' HP pots left /  ', player getManaPot asString, ' Mana pots left' ; cr; cr.	"Transcript showing current status for given player."! !!GameArena class methodsFor: 'as yet unclassified' stamp: 'Deniz 5/19/2017 21:08'!attack: firstPlayer defence: secondPlayer	firstPlayer setHP: firstPlayer getHP - (secondPlayer getATK * (1 - (0.06 * firstPlayer getDEF)/(1+(0.